{
  "display": {
    "window_width": 1600,
    "window_height": 900,
    "render_fps": 60,
    "fullscreen": false,
    "vsync": true,
    "hardware_acceleration": true,
    "double_buffer": true,
    "anti_aliasing": false,
    "texture_filtering": "nearest"
  },
  "audio": {
    "music_volume": 0.7,
    "sfx_volume": 0.8,
    "voice_volume": 0.6,
    "enable_music": true,
    "enable_sfx": true,
    "enable_voice": true,
    "audio_channels": 32,
    "sample_rate": 44100
  },
  "gameplay": {
    "difficulty": "normal",
    "auto_save_interval": 300,
    "max_save_slots": 10,
    "enable_cheats": false,
    "enable_debug": false,
    "language": "en",
    "subtitles": true,
    "tutorial_enabled": true
  },
  "performance": {
    "enable_vsync": true,
    "max_fps": 60,
    "min_fps": 30,
    "enable_frame_skipping": true,
    "enable_lod": true,
    "texture_quality": "high",
    "shadow_quality": "medium",
    "particle_limit": 1000,
    "enable_occlusion_culling": true,
    "enable_frustum_culling": true,
    "batch_size": 1000,
    "enable_multithreading": true,
    "thread_count": 4
  },
  "graphics": {
    "renderer": "opengl",
    "shader_quality": "high",
    "post_processing": true,
    "bloom": true,
    "motion_blur": false,
    "depth_of_field": false,
    "ambient_occlusion": true,
    "screen_space_reflections": false,
    "volumetric_lighting": false,
    "enable_weather_effects": true,
    "max_light_sources": 16,
    "shadow_map_size": 2048,
    "enable_soft_shadows": true
  },
  "ai": {
    "ai_update_frequency": 30,
    "pathfinding_update_rate": 10,
    "max_ai_entities": 100,
    "ai_vision_range": 300,
    "ai_memory_duration": 60,
    "enable_ai_learning": true,
    "ai_difficulty_scaling": true,
    "max_ai_threads": 2
  },
  "physics": {
    "physics_fps": 60,
    "enable_physics": true,
    "gravity": 9.81,
    "collision_detection": "sweep_and_prune",
    "max_collision_objects": 1000,
    "enable_ragdoll": true,
    "enable_soft_bodies": false,
    "enable_fluid_simulation": false
  },
  "network": {
    "enable_multiplayer": false,
    "max_players": 4,
    "network_tick_rate": 20,
    "enable_lan": true,
    "enable_internet": false,
    "connection_timeout": 30,
    "max_ping": 200
  },
  "input": {
    "mouse_sensitivity": 1.0,
    "gamepad_enabled": true,
    "keyboard_layout": "qwerty",
    "enable_mouse_acceleration": false,
    "invert_mouse_y": false,
    "enable_auto_aim": false,
    "deadzone": 0.1
  },
  "ui": {
    "ui_scale": 1.0,
    "enable_animations": true,
    "enable_transitions": true,
    "font_quality": "high",
    "enable_hud": true,
    "enable_minimap": true,
    "enable_quest_log": true,
    "enable_inventory": true,
    "ui_theme": "dark"
  },
  "audio_processing": {
    "enable_3d_audio": true,
    "audio_reverb": true,
    "audio_echo": false,
    "audio_compression": true,
    "audio_streaming": true,
    "buffer_size": 1024,
    "enable_audio_filters": true
  },
  "memory": {
    "enable_memory_pooling": true,
    "max_memory_usage": 2048,
    "enable_garbage_collection": true,
    "gc_frequency": 300,
    "enable_object_pooling": true,
    "max_object_pool_size": 10000,
    "enable_texture_streaming": true,
    "texture_cache_size": 512
  },
  "debug": {
    "enable_logging": true,
    "log_level": "info",
    "enable_profiling": false,
    "enable_memory_tracking": false,
    "enable_performance_monitoring": true,
    "enable_error_reporting": true,
    "save_debug_info": false,
    "enable_console": false
