  "accessibility": {
    "enable_color_blind_support": false,
    "enable_high_contrast": false,
    "enable_large_text": false,
    "enable_screen_reader": false,
    "enable_keyboard_navigation": true,
    "enable_voice_commands": false,
    "enable_motion_reduction": false
  }
}
